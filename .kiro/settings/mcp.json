{
  "mcpServers": {
    "postgresql": {
      "command": "node",
      "args": [
        "./postgresql-mcp-server/dist/index.js"
      ],
      "env": {
        "DATABASE_URL": "${DATABASE_URL}"
      },
      "disabled": false,
      "autoApprove": [
        "list_tables",
        "execute_query",
        "get_table_schema",
        "insert_data",
        "update_data",
        "delete_data"
      ]
    }
  }
}